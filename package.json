{
  "name": "ember-icons",
  "version": "0.0.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "demo": "pnpm turbo --filter test-app start",
    "prepare": "pnpm build",
    "build": "pnpm --filter ember-icons build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:tests": "pnpm --filter ember-icons start",
    "start:addon": "pnpm --filter ember-icons start --no-watch.clearScreen",
    "test": "pnpm --filter ember-icons test",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-icons",
    "test-app"
  ],
  "dependencies": {
    "turbo": "^1.6.2"
  }
}
