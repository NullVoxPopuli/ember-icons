{{#let this.entry as |entry|}}
  {{#if (this.isComponent entry)}}

    <entry.component fill="currentColor" ...attributes />

  {{else}}

    <svg ...attributes fill="currentColor">
      <use xlink:href={{entry.href}}></use>
    </svg>

  {{/if}}
{{/let}}

